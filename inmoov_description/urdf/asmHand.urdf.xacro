    <child link="${side}_index2"/>
  </joint>

  <joint name="${side}_index3" type="revolute">
   <axis xyz="1 0 0"/>
<limit effort="1000.0" lower="0" upper="1.5" velocity="0.5"/>
<origin rpy="0 0 0" xyz="0 0 -0.024"/>
