  <joint name="${side}_cover_pinky" type="fixed">
    <origin rpy="${neg * -0.05} ${neg * -0.1} 0" xyz="0 -0.005 -0.0105"/>
    <parent link="${side}_pinky2"/>
    <child link="${side}_cover_pinky"/>
