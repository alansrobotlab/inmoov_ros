  </link>

  <link name="${side}_pinky2">
    <visual>
      <geometry>
        <mesh filename="package://inmoov_meshes/meshes/pinky3_2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <origin rpy="0 0 3.1415" xyz="0 0 0"/>
