  <joint name="${side}_cover_ring" type="fixed">
    <origin rpy="${neg * -0.05} ${neg * -0.1} 0" xyz="0 -0.005 -0.011"/>
    <parent link="${side}_ring2"/>
    <child link="${side}_cover_ring"/>
